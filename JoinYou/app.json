{
  "expo": {
    "name": "JoinYou",
    "slug": "JoinYou",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/FN42_Logo_App_Size.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "The app accesses your photos to let you share them with your friends.",
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "@config-plugins/react-native-webrtc",
        {
          "cameraPermission": "Allow access your camera",
          "microphonePermission": "Allow to access your microphone"
        }
      ]
    ],
    "splash": {
      "image": "./assets/FN42_Logo_App_Size.png",
      "resizeMode": "contain",
      "backgroundColor": "#2c2c2c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ajeh.JoinYou"
    },
    "android": {
      "versionCode": 11,
      "adaptiveIcon": {
        "foregroundImage": "./assets/FN42_Logo_App_Size_Android.png",
        "backgroundColor": "#2c2c2c"
      },
      "permissions": ["READ_CALENDAR", "WRITE_CALENDAR"],
      "package": "com.ajeh.JoinYou"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
